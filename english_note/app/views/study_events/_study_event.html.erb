<div id="<%= dom_id study_event %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= study_event.study_event_type %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Word:</strong>
    <%= study_event.word.name %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">WordClass:</strong>
    <%= study_event.word.word_class %>
  </p>
</div>
